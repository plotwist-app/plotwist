{
  "name": "plotwist-api",
  "version": "1.14.1",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/main.ts",
    "build": "dotenv -e .env -- tsup src/main.ts --out-dir dist --format esm,cjs --dts",
    "start": "dotenv -e .env -- node dist/main.js",
    "db:seed": "tsx --env-file=.env src/db/seed.ts",
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:migrate:test": "dotenv -e .env.test -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "pretest": "pnpm run db:migrate:test",
    "test": "dotenv -e .env.test -- vitest",
    "test:ui": "dotenv -e .env.test -- vitest --ui",
    "test:coverage": "dotenv -e .env.test -- vitest --coverage",
    "format": "biome check src --write",
    "email:dev": "email dev --dir src/emails",
    "typecheck": "tsc --noEmit",
    "lint": "npx biome lint --write ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/client-sqs": "^3.699.0",
    "@aws-sdk/lib-storage": "^3.705.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/redis": "^7.0.1",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "@plotwist_app/tmdb": "0.1.2",
    "@react-email/components": "^0.0.32",
    "@swc/core": "^1.10.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.0.8",
    "@vitest/coverage-v8": "^2.1.8",
    "adm-zip": "^0.5.16",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "change-case": "^5.4.4",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "0.33.0",
    "drizzle-zod": "0.5.1",
    "env-paths": "2.2.1",
    "fast-xml-parser": "^4.5.0",
    "fastify": "^5.1.0",
    "fastify-type-provider-zod": "^4.0.2",
    "https": "^1.0.0",
    "ioredis": "^5.4.2",
    "node-cron": "^3.0.3",
    "openai": "^4.78.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postgres": "3.4.4",
    "puppeteer": "^24.1.0",
    "react": "^19.0.0",
    "react-dom": "19.0.0",
    "resend": "^4.0.1",
    "stripe": "^17.4.0",
    "typeid-js": "1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@faker-js/faker": "^9.3.0",
    "@types/adm-zip": "^0.5.7",
    "@types/bcrypt": "^5.0.2",
    "@types/ioredis-mock": "^8.2.5",
    "@types/node": "22.5.0",
    "@vitest/ui": "^2.1.8",
    "dotenv-cli": "7.4.2",
    "drizzle-kit": "0.24.1",
    "ioredis-mock": "^8.9.0",
    "localstack": "^1.0.0",
    "react-email": "3.0.6",
    "tsup": "8.2.4",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "^2.1.8",
    "zod": "3.23.8"
  }
}